  <div class="mini-cart">
    {%assign section.settings.type  = "type22"%}
    {{section.settings.type}}
    <div class="cart-container">
      <a href="javascript:;" class="icon-cart-header">
        <!--<svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-j-cart" viewBox="0 0 18 18"><path d="M9.027 0a3.519 3.519 0 0 0-3.512 3.512V4.83H3.759c-.353 0-.628.335-.659.597L2.002 17.28c-.034.364.293.722.659.72h12.732c.366.003.693-.356.658-.72L14.954 5.426a.682.682 0 0 0-.659-.597H12.54V3.512A3.519 3.519 0 0 0 9.027 0zm0 1.317c1.228 0 2.195.967 2.195 2.195V4.83h-4.39V3.512c0-1.228.967-2.195 2.195-2.195zm-4.665 4.83h9.33l.974 10.536H3.388l.974-10.537z"></path></svg>-->
		{% include 'icon-j-cart' %}
        <span class="cart-total"><span class="cart-qty">{{ cart.item_count }}</span><span >item(s)</span></span>
      </a> 
      <div class="cart-wrapper theme-border-color"> 
        <div class="cart-inner">
          <div class="cartloading" style="display: none;"><div class="spinner"><div class="circle"></div></div></div>
          <div class="cart-inner-content">
            <div class="cart-content">
              {% if cart.items == empty %}
              {% if section.settings.type == "type22" %}
              <div class="total-count">
                <span>{{ cart.item_count }} <span >item(s)</span></span>
                <a href="/cart" class="btn-button view-cart bordered uppercase">
                  <span >Cart</span>
                </a>
              </div>
              {% endif %}
              {% endif %}
              {% if cart.items != empty %}
              {% if section.settings.type == "type20" or section.settings.type == "type22" %}
              <div class="total-count">
                <span>{{ cart.item_count }} <span >item(s)</span></span>
                <a href="/cart" class="btn-button view-cart bordered uppercase">
                  <span >Cart</span>
                </a>
              </div>
              {% endif %}
              <ul class="clearfix">
                {% for item in cart.items %}
                <li class="item-cart">
                  <a class="product-image" href="{{item.url }}">
                    <img src="{{ item | img_url: 'small' }}"  alt="{{ item.title | escape }}" />
                  </a>
                  <div class="product-details row-fluid show-grid">
                    <p class="product-name">
                      <a href="{{item.url }}">
                        <span> 

                          {{ item.product.title | split: '|' | first }}
                        </span>
                        {% if item.product.variants.size > 1 %}
                        <span class="variant_title"> - {{ item.variant.title }}</span>
                        {% endif %}
                      </a>
                    </p>
                    <div class="items">
                      <span class="price">{{ item.quantity }} Ã— <span class="amount"><span class="money">{% if item.original_price != item.price %}
                  <del class="original-price">{{ item.original_price | money }}</del>
                  {% endif %}{{ item.price | money }}</span></span></span>
                    </div> 
                    <div class="access">
                      <a href="javascript:;" class="btn-remove btn-remove-cart" data-id="{{item.variant_id}}" title="remove"><i class="icon-cancel"></i></a>
                    </div>
                  </div> 
                </li>
                {% endfor %}

                
              </ul>
              
              {% else %}
              <p class="no-items-in-cart" >Cart is empty!</p>
              {% endif %}
            </div> 
            {% if cart.items != empty %}
            <div class="cart-checkout">
              <div class="cart-info"> 
                <p class="subtotal">
                  <span class="label" >Total:</span>
                  <span class="price"><span class="money">{{ cart.total_price | money }}</span></span>
                </p>
              </div>
              {% if section.settings.type == 'type20' or section.settings.type == 'type22' %}
              <div class="actions">
                <a href="/checkout" class="btn-button bordered uppercase checkout-cart">
                  <span >Checkout</span>
                </a>
              </div>
              {% else %}
              <div class="actions">
                <a href="/cart" class="btn-button view-cart bordered uppercase">
                  <span >Cart</span>
                </a>
                <a href="/checkout" class="btn-button bordered uppercase checkout-cart">
                  <span >Checkout</span>
                </a>
              </div>
              {% endif %}
            </div>
            {% endif %}
          </div>
        </div>
      </div>
    </div>
  </div> 
</div>

